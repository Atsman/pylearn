    "def binary_iter(n, arr, i):\n",
    "    if i == n:\n",
    "        print(arr)\n",
    "    else:\n",
    "        arr[i] = 0\n",
    "        binary_iter(n, arr, i + 1)\n",
    "        arr[i] = 1\n",
    "        binary_iter(n, arr, i + 1)\n",
    "        \n",
    "def binary(n):\n",
    "    arr = [None]*n\n",
    "    binary_iter(n, arr, 0)"
   ]
